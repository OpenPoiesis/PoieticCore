    {
      "content" : [
        {
          "anchor" : "discussion",
          "level" : 2,
          "text" : "Discussion",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Only a point value and certain string values can be converted to a point.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The point value is represented as a string with the ",
              "type" : "text"
            },
            {
              "code" : "x",
              "type" : "codeVoice"
            },
            {
              "text" : " character as",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "point component value separator, so for example point ",
              "type" : "text"
            },
            {
              "code" : "Point(x:10, y:20)",
              "type" : "codeVoice"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "is represented as string as ",
              "type" : "text"
            },
            {
              "code" : "\"10.0x20.0\"",
              "type" : "codeVoice"
            },
            {
              "text" : " (note that the point components",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "are doubles). The conversion is as follows:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "The string is split at the first ",
                      "type" : "text"
                    },
                    {
                      "code" : "x",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " character",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "If there are not exactly two values - an error is thrown",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Each of the two values is converted to a double, if the conversion fails",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "an error is thrown.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "The first value is the ",
                      "type" : "text"
                    },
                    {
                      "code" : "x",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " component of the point and the second value",
                      "type" : "text"
                    },
                    {
                      "text" : " ",
                      "type" : "text"
                    },
                    {
                      "text" : "is the ",
                      "type" : "text"
                    },
                    {
                      "code" : "y",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " component of the point.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "",
                  "type" : "text"
                },
                {
